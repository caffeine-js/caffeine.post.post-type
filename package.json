{
    "name": "@caffeine-packages/post.post-type",
    "author": {
        "name": "Alan Reis",
        "email": "alanreisanjo@gmail.com",
        "url": "https://hoyasumii.dev"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./*": {
            "types": "./dist/*/index.d.ts",
            "import": "./dist/*/index.js",
            "require": "./dist/*/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "scripts": {
        "build": "tsup 'src/**/index.ts' --format cjs,esm --dts --tsconfig tsconfig.build.json --clean",
        "test:unit": "bun test --env-file=.env.testing",
        "test:coverage": "bun test --env-file=.env.testing --coverage",
        "setup": "bun run build && bun link",
        "start:dev": "bun --watch src/presentation/dev/index.ts",
        "prepare": "husky"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.4.1",
        "@commitlint/config-conventional": "^20.4.1",
        "@elysiajs/eden": "^1.4.8",
        "@faker-js/faker": "^10.2.0",
        "@types/bun": "latest",
        "dotenv": "^17.3.1",
        "elysia": "^1.4.26",
        "husky": "^9.1.7",
        "tsup": "^8.5.1"
    },
    "peerDependencies": {
        "typescript": "^5",
        "elysia": "^1.4.26"
    },
    "dependencies": {
        "@caffeine-adapters/post": "link:@caffeine-adapters/post",
        "@caffeine/api-docs": "link:@caffeine/api-docs",
        "@caffeine/api-error-handler": "link:@caffeine/api-error-handler",
        "@caffeine/api-response-mapper": "link:@caffeine/api-response-mapper",
        "@caffeine/application": "link:@caffeine/application",
        "@caffeine/auth": "link:@caffeine/auth",
        "@caffeine/constants": "link:@caffeine/constants",
        "@caffeine/domain": "link:@caffeine/domain",
        "@caffeine/entity": "link:@caffeine/entity",
        "@caffeine/errors": "link:@caffeine/errors",
        "@caffeine/models": "link:@caffeine/models",
        "@caffeine/presentation": "link:@caffeine/presentation",
        "@caffeine/cache": "link:@caffeine/cache",
        "@caffeine/schema": "link:@caffeine/schema",
        "@caffeine/env": "link:@caffeine/env",
        "@caffeine/value-objects": "link:@caffeine/value-objects"
    }
}
